  "workbench.statusBar.visible": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "workbench.layoutControl.enabled": false,
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "poimandres",
  "workbench.preferredLightColorTheme": "Min Dark",
  "gitlens.advanced.messages": {
    "suppressIntegrationRequestTimedOutWarning": true
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "grunt.autoDetect": "on",
  "jake.autoDetect": "on",
  "gulp.autoDetect": "on",
  /* LINGUAGEM R */
  "r.bracketedPaste": true,
  "r.plot.useHttpgd": true,
  /*   "r.rpath.windows": "C:\\Program Files\\R\\bin\\R.exe", */
  "r.rpath.windows": "C:\\Users\\cesar.oliveira\\AppData\\Local\\Programs\\R\\R-4.4.1\\bin\\R.exe",
